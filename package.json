{
  "name": "saasmaster-api",
  "version": "1.0.0",
  "description": "API for SaaSMaster app",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=dev node server.js",
    "start:dev": "NODE_ENV=dev node server.js",
    "start:prod": "NODE_ENV=prod node server.js",
    "build-spa": "cd ../saasmaster && npm run build",
    "copy": "cp -r ../saasmaster/build/* build",
    "build": "gcloud builds submit --tag gcr.io/saasmaster/saasmaster-api",
    "deploy": "gcloud beta run deploy saasmaster-api --image gcr.io/saasmaster/saasmaster-api --platform managed",
    "push": "npm run build-spa && npm run copy && npm run build && npm run deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Omri Gazitt",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^2.5.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "facebook-nodejs-business-sdk": "^5.0.1",
    "jwks-rsa": "^1.6.0"
  }
}
